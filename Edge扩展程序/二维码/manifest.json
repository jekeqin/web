{
    "name": "二维码",
    "description": "快速生成网址二维码",
    "manifest_version": 2,
    "version": "0.0.0.1",
    "icons":{
        "16": "icons/16x16.png",
        "32": "icons/32x32.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "browser_action":{
        "default_popup": "popup/code.html"
    },
    "content_scripts": [
        {
            "js": ["source/import.js"],
            "matches":[
                "<all_urls>"
            ]
        }
    ]
}